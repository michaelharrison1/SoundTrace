{
  "version": 2,
  "builds": [
    {
      "src": "../backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/vite"
    }
  ],
  "routes": [
    { "src": "/api/auth/(.*)", "dest": "../backend/server.js" },
    { "src": "/api/auth", "dest": "../backend/server.js" },
    { "src": "/api/scanlogs/(.*)", "dest": "../backend/server.js" },
    { "src": "/api/scanlogs", "dest": "../backend/server.js" },
    { "src": "/api/spotify-track-details", "dest": "../backend/server.js" },
    { "src": "/api/scan-track", "dest": "/api/scan-track" },
    { "src": "/api/spotify-artist-details", "dest": "/api/spotify-artist-details" },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}